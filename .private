#!/bin/bash

# my personal configuration

#export PATH
#unset USERNAME
#umask 002

export MOZILLA_FIVE_HOME=""
export HISTSIZE=100000
export HISTFILESIZE=100000

# tell bash to understand emacs commands
set -o emacs

alias rm='rm -i'
alias mv='mv -i'
alias ll='ls -l'
alias la='ls -la'

source ~/.function

alias e='emq'
alias E='emacs'
alias c='clear'
alias j='jobs'

# using screen
alias sc='screen'
alias scr='screen -r'
